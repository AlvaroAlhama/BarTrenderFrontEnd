# Generated by Selenium IDE
import pytest
import time
import json
from selenium import webdriver
from selenium.webdriver.common.by import By
from selenium.webdriver.common.action_chains import ActionChains
from selenium.webdriver.support import expected_conditions
from selenium.webdriver.support.wait import WebDriverWait
from selenium.webdriver.common.keys import Keys
from selenium.webdriver.common.desired_capabilities import DesiredCapabilities

class TestsPremiumDashboard():
  def setup_method(self, method):
    self.driver = webdriver.Chrome()
    self.vars = {}
  
  def teardown_method(self, method):
    self.driver.quit()
  
  def test_premiumDashboardNoPremiumUser(self):
    self.driver.get("https://bartrender-develop-sprint-01.netlify.app")
    self.driver.set_window_size(1051, 728)
    self.driver.find_element(By.ID, "login-tooltip").click()
    self.driver.find_element(By.ID, "email").send_keys("owner1@gmail.com")
    self.driver.find_element(By.NAME, "password").send_keys("vekto1234")
    self.driver.find_element(By.CSS_SELECTOR, ".modal-body").click()
    self.driver.find_element(By.ID, "email").send_keys("miggar@gmail.com")
    self.driver.find_element(By.CSS_SELECTOR, ".btn:nth-child(1)").click()
    element = self.driver.find_element(By.ID, "discount-tooltip")
    actions = ActionChains(self.driver)
    actions.move_to_element(element).perform()
    self.driver.find_element(By.ID, "discount-tooltip").click()
    self.driver.find_element(By.CSS_SELECTOR, ".nav > li:nth-child(2) p").click()
    self.driver.find_element(By.NAME, "zone").click()
    self.driver.find_element(By.NAME, "zone").send_keys("Cartuja")
    self.driver.find_element(By.ID, "filterImput").click()
    dropdown = self.driver.find_element(By.ID, "filterImput")
    dropdown.find_element(By.XPATH, "//option[. = 'Ocio']").click()
    self.driver.find_element(By.ID, "filterImput").click()
    self.driver.find_element(By.ID, "initialDate").click()
    self.driver.find_element(By.ID, "initialDate").send_keys("2021-04-15")
    self.driver.find_element(By.ID, "endDate").click()
    self.driver.find_element(By.ID, "endDate").send_keys("2021-04-18")
    self.driver.find_element(By.CSS_SELECTOR, ".btn:nth-child(4)").click()
    self.driver.find_element(By.LINK_TEXT, "Comprar Suscripción Premium").click()
    assert self.driver.find_element(By.CSS_SELECTOR, ".card-body h3").text == "Debe ser usuario premium para acceder a las estadísticas que buscas"
  
 

  def test_premiumDasboardSuccess(self):
    self.driver.get("https://bartrender-develop-sprint-01.netlify.app")
    self.driver.set_window_size(1051, 728)
    self.driver.find_element(By.ID, "login-tooltip").click()
    self.driver.find_element(By.ID, "email").send_keys("owner1@gmail.com")
    self.driver.find_element(By.NAME, "password").send_keys("vekto1234")
    self.driver.find_element(By.CSS_SELECTOR, ".btn:nth-child(1)").click()
    self.driver.find_element(By.CSS_SELECTOR, ".fal").click()
    self.driver.find_element(By.CSS_SELECTOR, ".nav > li:nth-child(2) p").click()
    self.driver.find_element(By.NAME, "zone").click()
    self.driver.find_element(By.NAME, "zone").send_keys("Cartuja")
    self.driver.find_element(By.ID, "initialDate").click()
    self.driver.find_element(By.ID, "initialDate").send_keys("2021-04-15")
    self.driver.find_element(By.ID, "endDate").click()
    self.driver.find_element(By.ID, "endDate").send_keys("2021-04-18")
    self.driver.find_element(By.CSS_SELECTOR, ".btn:nth-child(4)").click()
    self.driver.execute_script("window.scrollTo(0,0)")
    assert  self.driver.find_element(By.CSS_SELECTOR, "p:nth-child(6)") == "Datos obtenidos de la api de Bartrender"

  def test_premiumDashboardNoZone(self):
    self.driver.get("https://bartrender-develop-sprint-01.netlify.app")
    self.driver.set_window_size(1051, 728)
    self.driver.find_element(By.ID, "login-tooltip").click()
    self.driver.find_element(By.ID, "email").send_keys("owner1@gmail.com")
    self.driver.find_element(By.NAME, "password").send_keys("vekto1234")
    self.driver.find_element(By.CSS_SELECTOR, ".modal").click()
    self.driver.find_element(By.CSS_SELECTOR, ".modal").click()
    self.driver.find_element(By.ID, "email").click()
    self.driver.find_element(By.CSS_SELECTOR, ".modal").click()
    self.driver.find_element(By.CSS_SELECTOR, ".modal-body").click()
    self.driver.find_element(By.CSS_SELECTOR, ".modal").click()
    self.driver.find_element(By.NAME, "password").send_keys("vekto134")
    self.driver.find_element(By.NAME, "password").send_keys(Keys.ENTER)
    self.driver.find_element(By.CSS_SELECTOR, ".modal").click()
    self.driver.find_element(By.NAME, "password").send_keys("vekto1234")
    self.driver.find_element(By.NAME, "password").send_keys(Keys.ENTER)
    self.driver.find_element(By.CSS_SELECTOR, ".fal").click()
    self.driver.find_element(By.CSS_SELECTOR, ".nav > li:nth-child(2) p").click()
    self.driver.find_element(By.ID, "filterImput").click()
    self.driver.find_element(By.ID, "filterImput").click()
    self.driver.find_element(By.ID, "initialDate").click()
    self.driver.find_element(By.ID, "initialDate").send_keys("2021-04-16")
    self.driver.find_element(By.ID, "endDate").click()
    self.driver.find_element(By.ID, "endDate").send_keys("2021-04-18")
    self.driver.find_element(By.CSS_SELECTOR, ".btn:nth-child(4)").click()
    assert self.driver.find_element(By.CSS_SELECTOR, ".card-body > h3") =="no ha añadido la zona"



  def test_premiumDashboardNoDates(self):
    self.driver.get("https://bartrender-develop-sprint-01.netlify.app")
    self.driver.set_window_size(1051, 728)
    self.driver.find_element(By.ID, "login-tooltip").click()
